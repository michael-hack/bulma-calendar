{
  "name": "bulma-calendar",
  "description": "Display a calendar for date/time selection, in different colors and sizes",
  "main": "dist/js/bulma-calendar.js",
  "style": "dist/css/bulma-calendar.min.css",
  "sass": "src/sass/index.sass",
  "version": "6.0.7",
  "scripts": {
    "commit": "git-cz",
