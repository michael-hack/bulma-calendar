    "jekyll": "^3.0.0-beta1",
    "jest": "^23.1.0",
    "regenerator-runtime": "^0.11.1",
    "semantic-release": "^15.9.14",
    "webpack": "^4.12.0",
    "webpack-stream": "^4.0.3"
