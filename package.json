  "devDependencies": {
    "ansi-colors": "^2.0.1",
    "autoprefixer": "^8.6.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.23.6",
    "bulma": "^0.7.1",
    "camelcase": "^5.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "del": "^3.0.0",
    "event-stream": "^3.3.4",
    "fancy-log": "^1.3.2",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-clean-css": "^3.9.4",
    "gulp-concat": "^2.6.1",
    "gulp-nop": "0.0.3",
